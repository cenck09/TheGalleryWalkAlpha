@using Parse;
@model TheGalleryWalk.Models.GalleryEntity


@{
    ViewBag.Title = "OwnedArtwork";
}

<script src="@Url.Content("~/Scripts/jquery.jquery-1.5.1.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.unobtrusive-ajax.min.js")" type="text/javascript"></script>


<html>
<head>

</head>

<body>
    
    <div class="Info-header">
           <h3>
               My Artwork
           </h3>    
    </div> 
   
    <div style="text-align:center;">
        @using System.Diagnostics;
        @try
        {
            Html.RenderPartial("~/Views/Gallery/ArtworkPartialView.cshtml", (Model.ArtworkEntities as IEnumerable<ParseObject>));

            }
            catch (Exception ex)
            {
            Debug.WriteLine("There was an error ::    "+ex);
            }
</div>
    <div>
        
        <div class="add-btn" id="rmvGalleryBtn" style="display:none;">
            <h4>- Artist  </h4>
        </div>
         <div id="addArtworkForm" style="display:none">
           
             @if (Model == null)
             {
                @Html.Partial("~/Views/AddArtwork/Index.cshtml", new TheGalleryWalk.Models.ArtworkEntity());
             }
             else
             {
                @Html.Partial("~/Views/AddArtwork/Index.cshtml", Model.ArtworkAdd);
             }

         </div>
    </div>
</body>

</html>


